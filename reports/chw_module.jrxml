	<queryString>
		<![CDATA[SELECT 
  CONCAT_WS(" ", chw.first_name, chw.other_name, chw.second_name) AS name,
  district.name AS district,
  module.name AS module,
  module_progress.created_date AS dateReceived,
  module_progress.start_date AS startDate,
  module_progress.end_date AS endDate
 FROM module_progress
 JOIN module ON module.id = module_progress.module_id
 JOIN community_health_worker chw ON module_progress.chw_id = chw.id
 JOIN community ON chw.community_id = community.id
 JOIN facility ON community.facility_id = facility.id
 JOIN chiefdom ON facility.chiefdom_id = chiefdom.id
 JOIN district ON chiefdom.district_id = district.id
 ORDER BY name, module]]>
	</queryString>
